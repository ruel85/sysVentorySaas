<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ch.zbw.sysVentorySaas.App.model.Group" table="gruppe">
		<meta attribute="class-description">Diese Klasse enthält sämtliche Details zu einer Gruppe</meta>
		<id name="idGroup" type="int" column="idGroup">
			<generator class="identity"/>
		</id>
		<property name="name" column="name" type="string"/>
	</class>
</hibernate-mapping>